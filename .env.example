# Polyflow Configuration (example)
# Copy this to .env and fill in real values for your environment

# Database
DATABASE_URL=postgres://polyflow:YOUR_DB_PASSWORD@localhost:5432/polyflow

# Server
PORT=3000

# Rate limiting
GLOBAL_RATE_PER_SEC=10
GLOBAL_BURST=20

# Admin access (CHANGE THIS!)
ADMIN_API_KEY=changeme

# Logging
LOG_LEVEL=info